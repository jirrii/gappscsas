        var query = (sectionPageQuery.split('?').length > 1) ? sectionPageQuery.split('?')[1] : '';


        var section = sectionPage.split('/')[0];
        var page = (sectionPage.split('/').length > 1) ? sectionPage.split('/')[1] : '';


        $(document).ready(function() {

          //set active section
          $('#container').find('a[href=' + page + ']').addClass('active');

          $('#container a').each(function() {
            $(this).attr('href', siteUrl + '/' + section + '/' + $(this).attr('href') + (query ? ('?' + query): ''));
